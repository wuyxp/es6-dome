class Countdown{
    constructor(n){
        this.n = n;
        this.arr = [];
    }
    toArr(){
        while(this.n >= 0 ){
            this.arr.push(this.n--)
        }
    }
    getArr(){
        this.toArr();
        return this.arr
    }
}
let cnt = new Countdown(10);
let arr = cnt.getArr();
abc =1;
var ab =1;
var ab==2;
